@{
    Layout = "";
}
@model Nop.Plugin.Shipping.StoresPickup.Models.ConfigurationModel
@using Nop.Web.Framework;
@using (Html.BeginForm())
{

    <table class="adminContent">

        <tr>
            <td class="adminTitle">
                @Html.NopLabelFor(model => model.Tiendas):
            </td>
            <td class="adminData">
               
                @Html.EditorFor(model => model.Tiendas)
                @Html.ValidationMessageFor(model => model.Tiendas)
                
                
            </td>
        </tr>
        
        <tr>
            <td colspan="2" width="100%">
                <input type="submit" name="save" class="t-button" value="@T("Admin.Common.Save")" />
            </td>
        </tr>
    </table>    
}
